<h1> Create Song </h1>

<%= form_for @song do |f| %>
    <%= f.label "Song Name" %><br>
    <%= f.text_field 'name' %><br>

    <%= f.label "Genre id" %><br>
    <%= f.text_area 'genre_id' %><br>

    <%= f.label "Artist id" %><br>
    <%= f.text_area 'artist_id' %><br>

    <%= f.submit "Create Song" %>
<% end %>